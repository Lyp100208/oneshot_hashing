net: "./prototxt/cifar100/usual_train_24.prototxt"
test_iter: 40
test_interval: 100
base_lr: 0.01
momentum: 0.9
weight_decay: 0.0005
lr_policy: "step"
gamma: 0.1
stepsize: 60000
display: 100
max_iter: 180000
snapshot: 20000
snapshot_prefix: "./snapshot/cifar100/usual_24/usual24"
solver_mode: GPU
info {
  type: "weight"
  interval: 100
}
info {
  type: "mean_train_loss"
  interval: 100
}

