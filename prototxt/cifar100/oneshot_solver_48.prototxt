net: "./prototxt/cifar100/oneshot_train_48.prototxt"
test_iter: 5
test_interval: 1000
base_lr: 0.0001
momentum: 0.9
weight_decay: 0.0005
lr_policy: "step"
gamma: 0.1
stepsize: 6000
display: 1000
max_iter: 20000
snapshot: 2000
snapshot_prefix: "./snapshot/cifar100/oneshot_48/oneshot48"
solver_mode: GPU
info {
  type: "weight"
  interval: 1000
}
info {
  type: "mean_train_loss"
  interval: 1000
}

