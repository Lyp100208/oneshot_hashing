net: "./prototxt/cifar100/googlenet_support40_learnt_bilstm_train_48.prototxt"
test_iter: 20
test_interval: 100
base_lr: 0.001 # 0.01
momentum: 0.9
weight_decay: 0.0005
lr_policy: "step"
gamma: 0.1
stepsize: 60000#30000
display: 100
max_iter: 200000
snapshot: 10000
snapshot_prefix: "./snapshot/cifar100/googlenet_support40_learnt_bilstm_48/support40_learnt48_0.001"
solver_mode: GPU
info {
  type: "weight"
  interval: 100
}
info {
  type: "mean_train_loss"
  interval: 100
}

