net: "./prototxt/cifar100/support_learnt_train_48.prototxt"
test_iter: 3
test_interval: 100
base_lr: 0.01
momentum: 0.9
weight_decay: 0.0005
lr_policy: "step"
gamma: 0.1
stepsize: 30000
display: 100
max_iter: 200000
snapshot: 20000
snapshot_prefix: "./snapshot/cifar100/support_learnt_48/support_learnt48"
solver_mode: GPU
info {
  type: "weight"
  interval: 100
}
info {
  type: "mean_train_loss"
  interval: 100
}

