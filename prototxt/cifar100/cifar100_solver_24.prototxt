net: "./prototxt/cifar100/cifar100_train_24.prototxt"
test_iter: 100
test_interval: 10000
base_lr: 0.01
momentum: 0.9
weight_decay: 0.0005
lr_policy: "step"
gamma: 0.1
stepsize: 60000
display: 500
max_iter: 130000
#snapshot: 10000
#snapshot_prefix: "models/cifar10_24"
solver_mode: GPU
info {
  type: "weight"
  interval: 100
}
info {
  type: "mean_train_loss"
  interval: 100
}

